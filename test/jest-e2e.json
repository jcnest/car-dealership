{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
        "^@/(.*)$": "<rootDir>/../src/$1",
        "^@cars/(.*)$": "<rootDir>/../src/cars/$1",
        "^@brands/(.*)$": "<rootDir>/../src/brands/$1",
        "^@seed/(.*)$": "<rootDir>/../src/seed/$1"
    }
}